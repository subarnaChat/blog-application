<%- include("partials/header");-%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="exampleInputTitle">Title</label>
    <input type="text" class="form-control" id="exampleInputTitle"  name="newTitle" placeholder="Enter title">
  </div>

  <div class="form-group">
   <label for="exampleContent">Content</label>
  <textarea name="newContent" class="form-control" rows="8" cols="80" id="exampleContent"></textarea>
 </div>

  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>


<%- include("partials/footer");-%>
